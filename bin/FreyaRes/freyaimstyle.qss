QToolButton#IDCGenKeepBtn, QToolButton#IDCMaleBtn, QToolButton#IDCFemaleBtn
{
    border: 1px solid green;
}
QToolButton#IDCGenKeepBtn:checked, QToolButton#IDCMaleBtn:checked, QToolButton#IDCFemaleBtn:checked
{
    border: 1px solid red;
}
QToolButton#IDCGenKeepBtn:checked:disabled, QToolButton#IDCMaleBtn:checked:disabled, QToolButton#IDCFemaleBtn:checked:disabled
{
    border: 1px solid blue;
}